{
    "name": "ev-vscode",
    "displayName": "ev-vscode",
    "description": "",
    "version": "0.0.5",
    "repository": {
        "url": "https://github.com/evalldor/ev-vscode"
    },
    "publisher": "evalldor",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "ev.mark.set",
                "title": "EV: Set Mark"
            },
            {
                "command": "ev.mark.cancel",
                "title": "EV: Cancel Mark"
            },
            {
                "command": "ev.cursor.move",
                "title": "EV: cursorMove"
            },
            {
                "command": "ev.scroll",
                "title": "EV: scroll"
            },
            {
                "command": "ev.scroll.toCursor",
                "title": "EV: Reposition view"
            },
            {
                "command": "ev.filepicker.open",
                "title": "EV: Open filepicker"
            },
            {
                "command": "ev.filepicker.toggleMode",
                "title": "EV: Toggle filepicker actions"
            },
            {
                "command": "ev.filepicker.goUpOneLevel",
                "title": "EV: Go to parent directory in filepicker"
            },
            {
                "command": "ev.test",
                "title": "EV: test"
            }
        ],
        "configuration": {
            "title": "EV",
            "properties": {
                "ev.filepicker.matchingThreshold": {
                    "type": "number",
                    "default": 0.49,
                    "description": "Don't show results with a score higher than this. Value between 0 and 1"
                }
            }
        },
        "keybindings": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@types/vscode": "^1.80.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.2",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "fuse.js": "^6.6.2"
    }
}
